
<main class="mdl-layout__content">
    <div class="page-content"><!-- Your content goes here -->
  
<h1 class="textcenter"> <b>All Users</b></h1>
<div class="mdl-color--grey-100">
<%= will_paginate %>

<div class="mdl-grid">

			  		<div class="mdl-cell mdl-cell--4-col">
			  		</div>


<br/ >

<div class="mdl-cell mdl-cell--3-col">
  <% @users.each do |user| %>


<div class="demo-card-square mdl-card mdl-shadow--2dp ">

<%=  image_tag user.profilepic.url(:thumbnail),class: "cnimg" if user.profilepic?  %>
  <div class="mdl-card__title mdl-card--expand">
    <h2 class="mdl-card__title-text"></h2>
  </div>
  <h3 class="textcenteruser"><b>&nbsp&nbsp&nbsp<%= user.username %></b></h3>
  
  <div class="mdl-card__actions mdl-card--border">
    
      <%= link_to "View Profile", user_path(user), class: "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
    
  </div>
</div>
<br/ >
    
    
  <% end %>
  </div>
  <div class="mdl-cell mdl-cell-5-col">
  </div>
  </div>
  
 </div>
</div>
</main>