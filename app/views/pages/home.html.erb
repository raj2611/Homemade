<html>
	<head>
		<title>Homemade</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
	
		<!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->
		<!--[if lte IE 9]><link rel="stylesheet" href="assets/css/ie9.css" /><![endif]-->
	</head>
	<body class="landing">

		<!-- Header -->
			<header id="header" class="alt">
				<h1><a href="index.html">HomeMade</a></h1>
				
				<a href="#nav">Menu</a>
			</header>

		<!-- Nav -->
			<nav id="nav">
				<ul class="links">
					<li><%= link_to "Sign Up",  new_user_registration_path %></li>
					<li><%= link_to "Log In",  new_user_session_path %></li>
					<li><a href="elements.html">Contact Me</a></li>
				</ul>
			</nav>

		<!-- Banner -->
			<section id="banner">
<i class="material-icons icon">home</i>
				
				<h2>Welcome To HomeMade</h2>
				<p>Start Your Journey</p>
				<ul class="actions">
					<li><%= link_to "Sign Up",  new_user_registration_path,class: "button big special" %></li>
					<li><%= link_to "Log In",  new_user_session_path,class: "button big special" %></li>
				</ul>
			</section>

		<!-- One -->
			<section id="one" class="wrapper style1">
				<div class="inner">
					<article class="feature left">
						<span class="image"><%= image_tag("pic01.jpg") %></span>
						<div class="content">
							<h2>Already have an account?</h2>
							<p>Log in and Create Like Browse And much more</p>
							<ul class="actions">
								<li>
									<li><%= link_to "Log In",  new_user_session_path,class: "button alt" %></li>
								</li>
							</ul>
						</div>
					</article>
					<article class="feature right">
						<span class="image"><%= image_tag("pic02.jpg") %></span>
						<div class="content">
							<h2>Only for a Tour?</h2>
							<p>No Problem. Browse and take a tour of what this contains You can like or comment or create.</p>
							<ul class="actions">
								<li>
									<%= link_to "Demo Tour",  contents_path,class: "button alt" %>
								</li>
							</ul>
						</div>
					</article>
				</div>
			</section>

		<!-- Two -->
			<section id="two" class="wrapper special">
				<div class="inner">
					<header class="major narrow">
						<h2>We Got You Covered</h2>
						<p>Everything You will see..</p>
					</header>
					<div class="image-grid">
						<a href="#" class="image"><%= image_tag("pic03.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic04.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic05.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic06.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic07.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic08.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic09.jpg") %></a>
						<a href="#" class="image"><%= image_tag("pic10.jpg") %></a>
					</div>
					<ul class="actions">
						<li><a href="#" class="button big alt">See More</a></li>
					</ul>
				</div>
			</section>

		<!-- Three -->
			<section id="three" class="wrapper style3 special">
				<div class="inner">
					<header class="major narrow	">
						<h2>Contact me </h2>
						<p>Ipsum dolor tempus commodo turpis adipiscing Tempor placerat sed amet accumsan</p>
					</header>
					<ul class="actions">
						<li><a href="#" class="button big alt">Contact Us</a></li>
					</ul>
				</div>
			</section>

		<!-- Four -->
			

		<!-- Footer -->
			<footer id="footer">
				<div class="inner">
					<ul class="icons">
						<li><a href="#" class="icon fa-facebook">
							<span class="label">Facebook</span>
						</a></li>
						<li><a href="#" class="icon fa-twitter">
							<span class="label">Twitter</span>
						</a></li>
						<li><a href="#" class="icon fa-instagram">
							<span class="label">Instagram</span>
						</a></li>
						<li><a href="#" class="icon fa-linkedin">
							<span class="label">LinkedIn</span>
						</a></li>
					</ul>
					<ul class="copyright">
						<li>&copy; Sumant Raj.</li>
						<li>Website: <a href="">Homemade</a>.</li>
						<li>Design and code: <a href="">Sumant Raj</a>.</li>
					</ul>
				</div>
			</footer>

		<!-- Scripts -->
			
			<!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
			

	</body>
</html>
